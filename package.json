{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "A good enough starter kit for a React project, I guess.",
  "scripts": {
    "start": "webpack && node ./app.js",
    "backend": "node ./app.js",
    "frontend": "webpack --progress --colors --watch",
    "build": "webpack --config ./webpack.config.js && cp bundle.js ./public/bundle.js",
